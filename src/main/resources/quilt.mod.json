{
  "schema_version": 1,
  "quilt_loader": {
    "group": "io.github.mg138",
    "id": "ijo_pona_poki",
    "version": "${version}",
    "metadata": {
      "name": "ijo pona poki (AE2Things rewrite)",
      "description": "Complete rewrite of the fabric mod AE2Things.",
      "contributors": {
        "Madeline Sparkles": "Owner"
      },
      "contact": {
        "homepage": "https://github.com/Mg138/ijo-pona-poki/",
        "issues": "https://github.com/Mg138/ijo-pona-poki/issues",
        "sources": "https://github.com/Mg138/ijo-pona-poki/"
      },
      "icon": "assets/ijo_pona_poki/icon.png"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "ae2": {
        "adapter": "kotlin",
        "value": "io.github.mg138.ijo_pona_poki.IjoPonaPokiAE2"
      },
      "ae2:client": {
        "adapter": "kotlin",
        "value": "io.github.mg138.ijo_pona_poki.IjoPonaPokiAE2Client"
      },
      "init": {
        "adapter": "kotlin",
        "value": "io.github.mg138.ijo_pona_poki.IjoPonaPoki"
      },
      "client_init": {
        "adapter": "kotlin",
        "value": "io.github.mg138.ijo_pona_poki.IjoPonaPokiClient"
      },
      "emi": {
        "adapter": "kotlin",
        "value": "io.github.mg138.ijo_pona_poki.IjoPonaPokiEmiPlugin"
      }
    },
    "depends": [
      {
        "id": "quilt_loader",
        "versions": ">=0.20.2-"
      },
      {
        "id": "quilted_fabric_api",
        "versions": ">=7.1.2-"
      },
      {
        "id": "qkl",
        "versions": ">=2.1.2-"
      },
      {
        "id": "minecraft",
        "versions": ">=1.20.1"
      },
      {
        "id": "ae2",
        "versions": ">=15.0.11"
      }
    ]
  }
}
